<html>
  <head>
    <meta charset="utf-8" />
    <title>UA Movie - Popular Movie Report</title>
    <link rel = "stylesheet" href = "/styles.css">
  </head>
  <body>
    <div>
      <center>
      <h1>View Popular Movie Report</h1>
    </center>
    </div>
    <table>
      <tr>
      <th>
        Month
      </th>
      <th>
        Movie
      </th>
      <th>
        # of Orders
      </th>
    </tr>
      <% for (var i = 0; i < data.length; i++) { %>
      <tr>
        <td>
          <% if (i == 0 || i == 3 || i == 6) { %>
            <%= data[i]['Month'] %>
          <% } %>
        </td>
        <td>
          <%= data[i]['title'] %>
        </td>
        <td>
          <%= data[i]['Orders'] %>
        </td>
      </tr>
      <% } %>
    </table>
    <form method = "post">
      <center><button type = "submit" formaction = "/back-manager">Back</button></center>
    </form>
  </body>
</html>
