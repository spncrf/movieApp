<html>
  <head>
    <meta charset="utf-8" />
    <title>UA Movie - Revenue Report</title>
    <link rel = "stylesheet" href = "/styles.css">
  </head>
  <body>
    <div>
      <center>
      <h1>View Revenue Report</h1>
    </center>
    </div>
    <table>
      <tr>
      <th>
        Month
      </th>
      <th>
        Revenue
      </th>
    </tr>
      <% for (var i = 0; i < data.length; i++) { %>
      <tr>
        <td>
          <%= data[i]['Month'] %>
        </td>
        <td>
          $<%= data[i]['Revenue'] %>
        </td>
      </tr>
      <% } %>
    </table>
    <form method = "post">
      <center><button type = "submit" formaction = "/back-manager">Back</button></center>
    </form>
  </body>
</html>
